<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Automation Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>

    <test name="AB Tests">
        <classes>
            <class name="tests.ABTestTest"/>
        </classes>
    </test>
</suite>
